{
  "version": "1.2.0",
  "manifest_version": 2,
  "name": "画像からカラーコードを取得",
  "description": "画像からカラーコードを取得",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [],
  "permissions": [
    "contextMenus",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "background",
    "activeTab",
    "*://*/*"
  ],
  "icons": {
    "16": "img/color-identify-16.png",
    "64": "img/color-identify-64.png",
    "96": "img/color-identify-96.png",
    "128": "img/color-identify-128.png"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://maxcdn.bootstrapcdn.com/; object-src 'self'"
}